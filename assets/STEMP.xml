      </courses>
    </program>

    <program name="Master of Science in Information Systems">
      <courses>
        <year number="1">
          <course code="SC400"/>
          <course code="Elective" description="Three IS400-level courses" prerequisite="SC400"/>
          <course code="Thesis" prerequisite="SC400 and three IS400-level courses"/>
        </year>
      </courses>
    </program>
  </programLevel>

