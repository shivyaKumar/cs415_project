        <year number="1">
          <course code="SC400"/>
          <course code="CS412" courseName="Artificial Intelligence" prerequisite="SC400"/>
          <course code="CS415" courseName="Advanced Software Engineering" prerequisite="SC400"/>
          <course code="CS424" courseName="Big Data Technologies" prerequisite="SC400"/>
          <course code="CS427" courseName="Mobile Comunications" prerequisite="SC400"/>
